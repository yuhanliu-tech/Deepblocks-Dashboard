    "            try:\n",
    "                signed_up_at.append(datetime.datetime.fromtimestamp(data_point['signed_up_at']).strftime('%Y-%m-%d'))\n",
    "            except:\n",
    "                signed_up_at.append(datetime.datetime.fromtimestamp(data_point['created_at']).strftime('%Y-%m-%d'))\n",
